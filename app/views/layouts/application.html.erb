<!DOCTYPE html>
<html>
<head>
  <title>NoRefunds</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="navbar">
    <%= display_navbar.html_safe %>
  </div>
  <% flash.each do |type, message| %>
    <%= content_tag :div, message.html_safe, class: "alert #{type}" %>
  <% end %>

<%= yield %>

</body>
</html>
